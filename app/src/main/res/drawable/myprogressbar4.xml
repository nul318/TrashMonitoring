    <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
        <item android:id="@android:id/background">
            <shape>
                <gradient

                    android:startColor="#f5ae98" android:centerColor="#f5ae98"
                    android:centerY="0.75" android:endColor="#f5ae98" android:angle="90" />

            </shape>
        </item>
        <item android:id="@android:id/progress">
            <clip>
                <shape>
                    <gradient android:startColor="#FFE6A28E" android:endColor="#FFE6A28E"
                        android:angle="90" />

                </shape>
            </clip>
        </item>
    </layer-list>
